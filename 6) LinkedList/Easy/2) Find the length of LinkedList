
// class Node{
//     int data;
//     Node next;
//     Node(int a){  data = a; next = null; }
// }


class Solution {
    // Function to count nodes of a linked list.
    public int getCount(Node head) {
        // code here
        Node pointerNode = head;
        int count = 0;
        while(pointerNode != null){
            count++;
            pointerNode = pointerNode.next;
        }
        
        return count;
    }
}


